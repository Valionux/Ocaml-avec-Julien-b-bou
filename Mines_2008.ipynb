     "data": {
      "text/plain": [
       "val deux_opt : 'a array -> int -> int -> 'a array = <fun>\n"
      ]
     },
     "execution_count": 15,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "let deux_opt t i j =\n",
    "    let i' = i + 1 in\n",
    "    let a1 = Array.sub t i' (j-i+1) in\n",
    "    for k = i' to j do\n",
    "        t.(k) <- a1.(j-k)\n",
    "    done;\n",
    "    t;;"
   ]
  },
